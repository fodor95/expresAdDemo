{% extends 'ExpresDemoAdminBundle:Page:main.html.twig' %}

{% block main -%}
    <h1>BlogCategories</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('blogcategories') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('blogcategories_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
