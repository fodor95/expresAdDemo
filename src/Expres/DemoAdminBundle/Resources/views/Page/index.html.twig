<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Administration{% endblock %}</title>
        
        {% block stylesheets %}

            <!-- ADMIN THEME EXTENSIONS -->
            	<!-- Bootstrap Core CSS -->
            <link href="{{ asset('admin/bower_components/bootstrap/dist/css/bootstrap.css') }}" type="text/css" rel="stylesheet" />

            	<!-- MetisMenu CSS -->
            <link href="{{ asset('admin/bower_components/metisMenu/dist/metisMenu.min.css') }}" type="text/css" rel="stylesheet" />

            	<!-- Timeline CSS -->
            <link href="{{ asset('admin/dist/css/timeline.css') }}" type="text/css" rel="stylesheet" />

            	<!-- Custom CSS -->
            <link href="{{ asset('admin/dist/css/sb-admin-2.css') }}" type="text/css" rel="stylesheet" />

            	<!-- Morris Charts CSS -->
            <link href="{{ asset('admin/bower_components/morrisjs/morris.css') }}" type="text/css" rel="stylesheet" />

            	<!-- Custom Fonts -->
            <link href="{{ asset('admin/bower_components/font-awesome/css/font-awesome.min.css') }}" type="text/css" rel="stylesheet" />

                

            <!-- END ADMIN THEME EXTENSIONS  -->
            
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>

    	{% block body %}{% endblock %}


        {% block javascripts %}
            
          

            
			<script src="{{ asset('admin/bower_components/jquery/dist/jquery.min.js') }}"> </script>
            <!-- ADMIN THEME EXTENSIONS -->
            <script src="{{ asset('admin/bower_components/bootstrap/dist/js/bootstrap.min.js') }}"> </script>

            	<!-- Metis Menu Plugin JavaScript -->
            	<script src="{{ asset('admin/bower_components/metisMenu/dist/metisMenu.min.js') }}"> </script>

            	<!-- Morris Charts JavaScript -->
            	<script src="{{ asset('admin/bower_components/raphael/raphael-min.js') }}"> </script>

                <!--
            	<script src="{{ asset('admin/bower_components/morrisjs/morris.js') }}"> </script>
            	<script src="{{ asset('admin/js/morris-data.js') }}"> </script>-->

            	<!-- Custom Theme JavaScript -->
            	<script src="{{ asset('admin/dist/js/sb-admin-2.js') }}"> </script>

                    <!-- FOR BOOTSTRAP TOOLTIPS -->
    
    
                <script src="{{ asset('admin/bower_components/bootstrap/js/tooltip.js') }}"> </script>

                <script src="{{ asset('bundles/stfalcontinymce/vendor/tinymce/tinymce.min.js') }}"> </script>
                           

                <script>
                // tooltip demo

                    $(document).ready(function() {
                        $("body").tooltip({ selector: '[data-toggle=tooltip]' });
                    });


                </script>


            <!-- END ADMIN THEME EXTENSIONS  -->
        {% endblock %}
    </body>
</html>
